# Backend Environment Variables (Production)
NODE_ENV=production
PORT=10000

# Frontend URL
FRONTEND_URL=https://deo-frontend.onrender.com

# API Keys (to be set in Render dashboard)
# OPENAI_API_KEY=your-openai-api-key-here
# SERPAPI_KEY=your-serpapi-key-here
# SCRAPEOWL_API_KEY=your-scrapeowl-api-key-here
# APOLLO_API_KEY=your-apollo-api-key-here
# HUNTER_API_KEY=your-hunter-api-key-here
# PEOPLEDATALABS_API_KEY=your-peopledatalabs-api-key-here
# PROSPEO_API_KEY=your-prospeo-api-key-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Settings
CACHE_TTL=3600
CACHE_MAX_KEYS=1000

# Timeouts
REQUEST_TIMEOUT=30000
OPENAI_TIMEOUT=45000
SCRAPING_TIMEOUT=30000

# Health Check
HEALTH_CHECK_ENDPOINT=/health

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true 